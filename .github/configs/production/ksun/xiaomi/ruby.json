{
  "kernelSource": {
    "device": "ruby",
    "vendor": "xiaomi",
    "name": "alan-kernel-ruby",
    "defconfig": "ruby_defconfig",
    "extraConfig": "",
    "repo": "https://github.com/alan7383/kernel_xiaomi_ruby",
    "branch": "main" 
  },
  "output_image": "Image",
  "kp": { "use": false, "kpimg_link": "" },
  "kernelsu": {
    "enable": true,
    "setupLink": "https://raw.githubusercontent.com/tiann/KernelSU/main/kernel/setup.sh",
    "setupArg": "main",
    "setupName": "KernelSU",
    "needPatch": false,
    "susfsLink": ""
  },
  "toolchains": [
    {
      "repo": "https://gitlab.com/RainyXeon/clang-r547379",
      "branch": "master",
      "name": "clang-AOSP",
      "binPath": ["/bin"]
    }
  ],
  "ccache": true,
  "params": {
    "ARCH": "arm64",
    "CC": "clang-AOSP/bin/clang",
    "externalCommand": {
      "KBUILD_BUILD_HOST": "github",
      "KBUILD_BUILD_USER": "alan7383",
      "CROSS_COMPILE": "clang-AOSP/bin/aarch64-linux-gnu-",
      "CROSS_COMPILE_ARM32": "clang-AOSP/bin/arm-linux-gnueabi-",
      "LD": "clang-AOSP/bin/ld.lld",
      "LLVM": "1",
      "LLVM_IAS": "1",
      "AR": "clang-AOSP/bin/llvm-ar",
      "NM": "clang-AOSP/bin/llvm-nm",
      "OBJCOPY": "clang-AOSP/bin/llvm-objcopy",
      "OBJDUMP": "clang-AOSP/bin/llvm-objdump",
      "READELF": "clang-AUMP/bin/llvm-readelf",
      "OBJSIZE": "clang-AOSP/bin/llvm-size",
      "STRIP": "clang-AOSP/bin/llvm-strip",
      "LDGOLD": "clang-AOSP/bin/aarch64-linux-gnu-ld.gold",
      "LLVM_AR": "clang-AOSP/bin/llvm-ar",
      "LLVM_DIS": "clang-AOSP/bin/llvm-dis"
    }
  },
  "image": { "use": false },
  "AnyKernel3": {
    "use": true,
    "release": true,
    "repo": "https://github.com/osm0sis/AnyKernel3",
    "branch": "master"
  }
}
